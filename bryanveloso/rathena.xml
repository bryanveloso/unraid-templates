<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/bryanveloso/unraid-templates/main/bryanveloso/rathena.xml</TemplateURL>
  <Beta>false</Beta>
  <Category>GameServers:</Category>
  <Name>bryanveloso-rathena</Name>
  <Overview>rAthena is an open-source cross-platform MMORPG server.</Overview>
  <Registry>https://registry.hub.docker.com/r/bryanveloso/rathena/</Registry>
  <GitHub>https://github.com/bryanveloso/docker-rathena/</GitHub>
  <Repository>bryanveloso/docker-rathena</Repository>
  <Project>https://github.com/rathena/rathena</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>6900</HostPort>
        <ContainerPort>6900</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6121</HostPort>
        <ContainerPort>6121</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5121</HostPort>
        <ContainerPort>5121</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Icon>https://rathena.org/board/uploads/monthly_2019_10/msapplication-square70x70logo.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
